.container
  .row
    .span4
    .span4
      .well
        %h4.form-signin-heading Please Enter Email
        = simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), class: 'form-signin') do |f|
          = f.input :email
          = f.submit 'Continue' , class: 'btn btn-primary'
    .span4
